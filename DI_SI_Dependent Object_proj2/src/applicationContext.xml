          </bean>
          
          <bean id="emp" class="com.nt.bean.Employee">
          <property name="id" value="1021"/>
          <property name="name" value="SriPatel" />
          <property name="phno" value="9010316499"/>
          <property name="address" ref="addr"/>
          </bean>
          
    </beans>  